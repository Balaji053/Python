   "source": [
    "# Ecludeian Power function log(n) time\n",
    "# a^b\n",
    "def power(a,b):\n",
    "    if b == 0:\n",
    "        return 1\n",
    "    half = power(a,b//2)\n",
    "    if b % 2 == 0:\n",
    "        return half * half\n",
    "    else:\n",
    "        return half * half * a\n",
    "    \n",
    "if __name__ == \"__main__\":\n",
    "    print(power(2,3)) # Output: 8"
   ]
